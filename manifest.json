{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
  ],
  "permissions": ["contextMenus"],
  "browser_action": {
  	"default_title": "4YM4N3 D0wnloader",
  	"default_popup": "popup.html"
  }
}